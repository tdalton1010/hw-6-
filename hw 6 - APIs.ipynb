    "# Dependencies\n",
    "import json\n",
    "import random\n",
    "import datetime\n",
    "import openweathermapy.core as owm\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd\n",
    "from citipy import citipy\n",
    "api_key = 
    "lat_list = []\n",
    "lon_list = []\n",
    "\n",
    "for x in range(100):\n",
    "    lat_list.append(random.uniform(-90,90))\n",
    "    lon_list.append(random.uniform(-180,180))\n",
    "\n",
    "lat_list"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 42,
   "metadata": {},
   "outputs": [],
   "source": [
    "cities = []\n",
    "settings = {\"units\": \"metric\", \"appid\": api_key}\n",
    "for i in range(10):\n",
    "    cities.append(citipy.nearest_city(lat_list[i], lon_list[i]))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 43,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
